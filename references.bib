@article{debski_designing_2016,
  title = {Designing and Interpretation of Digital Assays: {{Concentration}} of Target in the Sample and in the Source of Sample},
  issn = {2214-7535},
  shorttitle = {Designing and Interpretation of Digital Assays},
  doi = {10.1016/j.bdq.2016.04.002},
  abstract = {We explain how to design classic digital assays, comprising identical partitions, in order to obtain the required precision of the estimate within a defined range of concentrations. The design, including the number and volume of partitions, depends significantly on whether the assay is to assess the concentration of the target analyte in the sample or in the source of the sample (e.g. a patient body) with a given precision. We also show how to translate the result referring to the concentration in the sample into the concentration in the source of the sample, including the significant change in the breath of the confidence intervals.},
