  keywords = {Animals,Cells,Cultured,Gene Expression Profiling,Male,Messenger,MicroRNAs,Neurons,Patch-Clamp Techniques,Polymerase Chain Reaction,Rats,RNA,Single-Cell Analysis,Somatosensory Cortex,Wistar},
