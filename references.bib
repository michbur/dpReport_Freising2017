@article{george_detection_2013,
  title = {Detection and Quantification of Chimerism by Droplet Digital {{PCR}}},
  volume = {4},
  issn = {1938-1964},
  doi = {10.4161/chim.25400},
  abstract = {Accurate quantification of chimerism and microchimerism is proving to be increasingly valuable for hematopoietic cell transplantation as well as non-transplant conditions. However, methods that are available to quantify low-level chimerism lack accuracy. Therefore, we developed and validated a method for quantifying chimerism based on digital PCR technology. We demonstrate accurate quantification that far exceeds what is possible with analog qPCR down to 0.01\% with the potential to go even lower. Also, this method is inherently more informative than qPCR. We expect the advantages of digital PCR will make it the preferred method for chimerism analysis.},
